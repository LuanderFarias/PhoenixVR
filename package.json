{
  "name": "phoenixvr",
  "version": "0.1.2",
  "description": "A program to play vr with full-body and hand tracking with just a cellphone and a webcam!!",
  "main": "index.js",
  "author": {
    "name": "Luander De Faria",
    "email": "luanderdefaria475@gmail.com",
    "url": "https://github.com/LuanderFarias"
  },
  "scripts": {
    "test": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --windows --linux",
    "linux": "electron-builder --linux",
    "windows": "electron-builder --windows",
    "release": "electron-builder --windows --linux --publish always"
  },
  "build": {
    "appId": "com.LuanderFarias.PhoenixVR",
    "productName": "PhoenixVR",
    "linux": {
     "target": [
      "deb"
     ],
     "category": "Games"
    },
    "win": {
     "target": "NSIS",
     "icon": false
    }
   },
   "dependencies": {},
   "devDependencies": {
    "electron": "latest",
    "electron-builder": "^22.11.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuanderFarias/PhoenixVR.git"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/LuanderFarias/PhoenixVR/issues"
  },
  "homepage": "https://github.com/LuanderFarias/PhoenixVR#readme"
}